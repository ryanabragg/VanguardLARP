{
  "name": "vanguardlarp",
  "description": "website for VanguardLARP",
  "version": "0.0.0",
  "repository": {
    "type": "git",
    "url": "git://github.com/ryanabragg/VanguardLARP.git"
  },
  "homepage": "https://github.com/ryanabragg/VanguardLARP",
  "license": "MIT",
  "main": "src",
  "keywords": [
    "feathers"
  ],
  "author": {
    "name": "Ryan Bragg",
    "email": "ryan.a.bragg@gmail.com"
  },
  "contributors": [],
  "bugs": {},
  "directories": {
    "lib": "src"
  },
  "engines": {
    "node": ">= 6.0.0",
    "npm": ">= 3.0.0"
  },
  "scripts": {
    "preinstall": "npm i -g pm2",
    "test": "npm run eslint && npm run mocha",
    "eslint": "eslint src/. test/. --config .eslintrc.json",
    "mocha": "mocha test/ --recursive --compilers js:babel-core/register",
    "predev": "rm -f dist/*.js*",
    "dev": "BABEL_ENV=development NODE_ENV=development node src/",
    "build": "NODE_ENV=production node_modules/.bin/webpack --config config/webpack-production.js --progress --colors --hide-modules",
    "prestart": "npm run build",
    "start": "NODE_ENV=production node src/",
    "archive": "tar -czvf ../vanguardlarp.tar.gz ."
  },
  "dependencies": {
    "ajv": "^5.3.0",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "body-parser": "^1.17.1",
    "color": "^1.0.3",
    "compression": "^1.6.2",
    "cors": "^2.8.3",
    "dotenv": "^4.0.0",
    "feathers": "^2.1.1",
    "feathers-authentication": "^1.3.1",
    "feathers-authentication-hooks": "^0.1.2",
    "feathers-authentication-jwt": "^0.3.1",
    "feathers-authentication-local": "^0.4.4",
    "feathers-authentication-oauth2": "^0.3.2",
    "feathers-configuration": "^0.4.1",
    "feathers-errors": "^2.7.1",
    "feathers-hooks": "^2.1.2",
    "feathers-hooks-common": "^3.10.0",
    "feathers-nedb": "^2.6.2",
    "feathers-rest": "^1.8.1",
    "feathers-socketio": "^2.0.1",
    "file-loader": "^0.11.1",
    "helmet": "^3.5.0",
    "html-webpack-plugin": "^2.28.0",
    "localforage": "^1.5.0",
    "nedb": "^1.8.0",
    "passport-facebook": "^2.1.1",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-hot-loader": "^3.0.0-beta.7",
    "react-jss": "^6.1.1",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "serve-favicon": "^2.4.2",
    "styled-components": "^2.2.3",
    "uuid": "^3.1.0",
    "webpack": "^2.5.0",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "chai": "^4.0.2",
    "enzyme": "^2.9.1",
    "eslint": "^4.15.0",
    "eslint-config-google": "^0.9.1",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-react": "^7.5.1",
    "jsdom": "^11.5.1",
    "mocha": "^3.4.2",
    "react-addons-test-utils": "^15.6.0",
    "request": "^2.81.0",
    "sinon": "^4.1.4",
    "webpack-dev-middleware": "^1.10.2",
    "webpack-hot-middleware": "^2.18.0"
  }
}
